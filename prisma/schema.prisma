generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        String   @id @default(cuid())
  clerkUserId   String   @unique
  email String @unique
  name  String
}

model Record {
  id        String   @id @default(cuid())
  userId    String
  title     String
  audioUrl  String?
}
